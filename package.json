    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.4.0",
    "i18next-localstorage-backend": "^3.1.3",
    "keycloak-js": "^18.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.3",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-datepicker": "^4.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-ga": "^3.3.0",
    "react-hot-toast": "^2.2.0",
    "react-i18next": "^11.16.7",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.0",
    "react-table": "^7.7.0",
    "react-use": "^17.3.2"
  },
  "scripts": {
    "start": "dotenv -e .env craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "start:dev": "dotenv -e .env.development craco start",
    "start:prod": "dotenv -e .env.production craco start",
    "build:dev": "dotenv -e .env.development craco build",
    "build:prod": "dotenv -e .env.production craco build",
    "prettier": "prettier --config ./.prettierrc.json --write \"./src/**/*.+(js|jsx)\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx src",
    "test": "craco test",
    "test:ci": "craco test --coverage --watchAll=false",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:update-snapshot": "craco test --updateSnapshot"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-plugin-import": "^2.26.0",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "preval.macro": "^5.0.0",
    "tailwindcss": "^3.0.24",
    "twin.macro": "^2.8.2",
    "typescript": "^4.6.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/serviceWorker.js",
      "!src/**/index.js",
      "!src/**/*.prod.js",
      "!src/keycloak/*"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}