{
  "name": "eco-ssot-frontend",
  "version": "3.1.4",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@tippyjs/react": "^4.2.6",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.23.0",
    "echarts": "5",
    "i18next": "^21.4.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.3.1",
    "i18next-localstorage-backend": "^3.1.3",
    "keycloak-js": "^15.0.1",
    "lodash": "^4.17.21",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-hot-toast": "^2.1.1",
    "react-i18next": "^11.14.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5",
    "react-select": "^5.2.2",
    "react-table": "^7.7.0",
    "react-use": "^17.2.4"
  },
  "scripts": {
    "start": "cross-env TAILWIND_MODE=watch dotenv -e .env craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "start:dev": "cross-env TAILWIND_MODE=watch dotenv -e .env.development craco start",
    "start:prod": "cross-env TAILWIND_MODE=watch dotenv -e .env.production craco start",
    "build:dev": "dotenv -e .env.development craco build",
    "build:prod": "dotenv -e .env.production craco build",
    "prettier": "prettier --config ./.prettierrc.json --write \"./src/**/*.+(js|jsx)\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx src",
    "test": "craco test",
    "test:ci": "craco test --coverage --watchAll=false",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:update-snapshot": "craco test --updateSnapshot"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.2.0",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^8.4.1",
    "eslint-plugin-import": "^2.25.3",
    "msw": "^0.38.2",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.0.22",
    "twin.macro": "^2.7.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/serviceWorker.js",
      "!src/**/index.js",
      "!src/**/*.prod.js",
      "!src/keycloak/*"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
