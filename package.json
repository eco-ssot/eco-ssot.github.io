  "scripts": {
    "start": "dotenv -e .env vite",
    "build": "vite build",
    "eject": "react-scripts eject",
    "start:dev": "dotenv -e .env.development vite",
    "start:prod": "dotenv -e .env.production vite",
    "build:dev": "dotenv -e .env.development vite build",
    "build:prod": "dotenv -e .env.production vite build",
    "prettier": "prettier --config ./.prettierrc.json --write \"./src/**/*.+(js|jsx)\"",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.jsx src",
    "test": "vite test",
    "test:ci": "vite test --coverage --watchAll=false",
    "test:coverage": "vite test --coverage --watchAll=false",
    "test:update-snapshot": "vite test --updateSnapshot"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
