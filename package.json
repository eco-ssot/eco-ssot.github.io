  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/serviceWorker.js",
      "!src/**/index.js",
      "!src/**/*.prod.js"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
